{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "ES6+React16+Mobx+ESLint+Jest+Enzyme+Webpack3+SCSS",
  "main": "index.js",
  "author": "RohitRox",
  "license": "MIT",
  "scripts": {
    "build": "./node_modules/webpack/bin/webpack.js",
    "build:prod": "webpack --config webpack-prod.config.js",
    "watch": "./node_modules/webpack/bin/webpack.js --watch",
    "stat": "./node_modules/webpack/bin/webpack.js --eslint-config-airbnbig webpack.stats.config.js",
    "lint": "./node_modules/eslint/bin/eslint.js -c .eslintrc.json ./src/js",
    "test": "./node_modules/jest-cli/bin/jest.js"
  },
  "jest": {
    "rootDir": "./src",
    "setupTestFrameworkScriptFile": "../tests/__test_support__",
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "node-sass": "^4.5.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "sass-loader": "^6.0.6",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.19.0",
    "webpack": "^3.6.0"
  },
  "devDependencies": {
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "react-test-renderer": "^16.0.0",
    "webpack-bundle-analyzer": "^2.9.0"
  }
}
